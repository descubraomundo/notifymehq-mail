{
    "name": "descubraomundo/notifymehq-mail",
    "description": "Provides a mail gateway for notifyme",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "mail"],
    "authors": [
        {
            "name": "Gabriel Passarelli",
            "email": "geeks@descubraomundo.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "graham-campbell/manager": "~2.0",
        "swiftmailer/swiftmailer": "~5.4",
        "soundasleep/html2text": "~0.2",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\NotifyMe\\": "notifyme/src/",
            "NotifyMeHQ\\Contracts\\": "contracts/src/",
            "NotifyMeHQ\\Laravel\\": "laravel/src/",
            "NotifyMeHQ\\Mail\\": "mail/src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
