{
    "name": "descubraomundo/notifymehq-mail",
    "description": "Provides a mail gateway for notifyme",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "mail"],
    "authors": [
        {
            "name": "Gabriel Passarelli",
            "email": "geeks@descubraomundo.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "swiftmailer/swiftmailer": "~5.4",
        "notifymehq/notifyme": "~1.0",
        "soundasleep/html2text": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyMeHQ\\Mail\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
